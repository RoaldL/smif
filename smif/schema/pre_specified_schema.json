{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Pre Specified Planning",
    "description": "List of Pre-specified Planning Commands",
    "type": "array",
    "items": {
        "$ref": "#/definitions/build_instruction"
    },
    "definitions": {
        "build_instruction": {
            "type": "object",
            "properties": {
                "asset_type": {
                    "type": "string"
                },
                "build_date": {
                    "type": "integer"
                },
                "attributes": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/key_value_pair"
                    }
                }
            },
            "required": ["asset_type", "build_date"]
        },
        "key_value_pair": {
            "type": "object",
            "properties": {
                "key": {
                    "type": "string"
                }
            },
            "required": ["key", "value"]
        }
    }
}
